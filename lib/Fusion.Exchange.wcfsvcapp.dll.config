<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.diagnostics>
    <trace>
      <listeners>
        <add type="Microsoft.WindowsAzure.Diagnostics.DiagnosticMonitorTraceListener, Microsoft.WindowsAzure.Diagnostics, Version=2.8.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" name="AzureDiagnostics">
          <filter type="" />
        </add>
      </listeners>
    </trace>
  </system.diagnostics>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
    <add key="EmailAddress" value="andre.mesquita@Fusion.com.br" />
    <add key="Password" value="Atecub001@" />
    <add key="AutoDiscover" value="https://outlook.office365.com/EWS/Exchange.asmx" />
    <add key="SalasEstaticas" value="sala.pequena@Fusion.com.br;sala.grande@Fusion.com.br" />
